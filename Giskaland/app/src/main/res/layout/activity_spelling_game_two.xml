<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/bakles3"
    tools:context="gl.giskaland.SpellingGame">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SpellingOut1"
        android:src="@drawable/sa"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:paddingLeft="50dp"
        android:paddingRight="20dp"
        />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SpellingOut2"
        android:src="@drawable/sa"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/SpellingOut1"
        android:layout_toEndOf="@+id/SpellingOut1"
        android:paddingLeft="20dp"
        android:paddingRight="20dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SpellingOut3"
        android:src="@drawable/sa"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/SpellingOut2"
        android:layout_toEndOf="@+id/SpellingOut2"
        android:paddingLeft="20dp"
        android:paddingRight="20dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SpellingOut4"
        android:src="@drawable/sa"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/SpellingOut3"
        android:layout_toEndOf="@+id/SpellingOut3"
        android:paddingLeft="20dp"
        android:paddingRight="20dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SpellingOut5"
        android:src="@drawable/sa"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/SpellingOut4"
        android:layout_toEndOf="@+id/SpellingOut4"
        android:paddingLeft="20dp"
        android:paddingRight="20dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SpellingOut6"
        android:src="@drawable/sa"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/SpellingOut5"
        android:layout_toEndOf="@+id/SpellingOut5"
        android:paddingLeft="20dp"
        android:paddingRight="20dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageOFitem"
        android:src="@drawable/api"
        android:layout_marginBottom="72dp"
        android:layout_above="@+id/SpellingOut5"
        android:layout_alignLeft="@+id/SpellingOut5"
        android:layout_alignStart="@+id/SpellingOut5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/TextSpellingLevel2Score"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/OutPutText"
        android:layout_above="@+id/imageOFitem"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="72dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ny mynd"
        android:id="@+id/newphoto"
        android:layout_centerVertical="true" />
</RelativeLayout>

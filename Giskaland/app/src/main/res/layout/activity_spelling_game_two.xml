<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="gl.giskaland.SpellingGame">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SpellingOut1"
        android:src="@drawable/sa"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SpellingOut2"
        android:src="@drawable/sa"
        android:layout_alignTop="@+id/SpellingOut1"
        android:layout_toRightOf="@+id/SpellingOut1"
        android:layout_toEndOf="@+id/SpellingOut1" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SpellingOut3"
        android:src="@drawable/sa"
        android:layout_alignTop="@+id/SpellingOut2"
        android:layout_toRightOf="@+id/SpellingOut2"
        android:layout_toEndOf="@+id/SpellingOut2" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SpellingOut4"
        android:src="@drawable/sa"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/SpellingOut3"
        android:layout_toEndOf="@+id/SpellingOut3" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SpellingOut5"
        android:src="@drawable/sa"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/SpellingOut4"
        android:layout_toEndOf="@+id/SpellingOut4" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SpellingOut6"
        android:src="@drawable/sa"
        android:layout_alignTop="@+id/SpellingOut5"
        android:layout_toRightOf="@+id/SpellingOut5"
        android:layout_toEndOf="@+id/SpellingOut5" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SpellingOut7"
        android:src="@drawable/sa"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/SpellingOut6"
        android:layout_toEndOf="@+id/SpellingOut6" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SpellingOut8"
        android:src="@drawable/sa"
        android:layout_alignTop="@+id/SpellingOut7"
        android:layout_toRightOf="@+id/SpellingOut7"
        android:layout_toEndOf="@+id/SpellingOut7" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SpellingOut9"
        android:src="@drawable/sa"
        android:layout_alignTop="@+id/SpellingOut8"
        android:layout_toRightOf="@+id/SpellingOut8"
        android:layout_toEndOf="@+id/SpellingOut8" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SpellingOut10"
        android:src="@drawable/sa"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/SpellingOut9"
        android:layout_toEndOf="@+id/SpellingOut9" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageOFitem"
        android:src="@drawable/api"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/TextSpellingLevel2Score"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/OutPutText"
        android:layout_below="@+id/imageOFitem"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ny mynd"
        android:id="@+id/newphoto"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="gl.giskaland.SpellingGameTwo">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Spelling2imageButton1"
        android:src="@drawable/sa"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Spelling2imageButton2"
        android:src="@drawable/sa"
        android:layout_alignTop="@+id/Spelling2imageButton1"
        android:layout_toRightOf="@+id/Spelling2imageButton1"
        android:layout_toEndOf="@+id/Spelling2imageButton1" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Spelling2imageButton3"
        android:src="@drawable/sa"
        android:layout_alignTop="@+id/Spelling2imageButton2"
        android:layout_toRightOf="@+id/Spelling2imageButton2"
        android:layout_toEndOf="@+id/Spelling2imageButton2" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Spelling2imageButton4"
        android:src="@drawable/sa"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/Spelling2imageButton3"
        android:layout_toEndOf="@+id/Spelling2imageButton3" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Spelling2imageButton5"
        android:src="@drawable/sa"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/Spelling2imageButton4"
        android:layout_toEndOf="@+id/Spelling2imageButton4" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Spelling2imageButton6"
        android:src="@drawable/sa"
        android:layout_alignTop="@+id/Spelling2imageButton5"
        android:layout_toRightOf="@+id/Spelling2imageButton5"
        android:layout_toEndOf="@+id/Spelling2imageButton5" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Spelling2imageButton7"
        android:src="@drawable/sa"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/Spelling2imageButton6"
        android:layout_toEndOf="@+id/Spelling2imageButton6" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Spelling2imageButton8"
        android:src="@drawable/sa"
        android:layout_alignTop="@+id/Spelling2imageButton7"
        android:layout_toRightOf="@+id/Spelling2imageButton7"
        android:layout_toEndOf="@+id/Spelling2imageButton7" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Spelling2imageButton9"
        android:src="@drawable/sa"
        android:layout_alignTop="@+id/Spelling2imageButton8"
        android:layout_toRightOf="@+id/Spelling2imageButton8"
        android:layout_toEndOf="@+id/Spelling2imageButton8" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Spelling2imageButton10"
        android:src="@drawable/sa"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/Spelling2imageButton9"
        android:layout_toEndOf="@+id/Spelling2imageButton9" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:src="@drawable/api"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textView2"
        android:layout_below="@+id/imageView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="65dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/button"
        android:layout_below="@+id/textView2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
